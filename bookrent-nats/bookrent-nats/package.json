{
  "name": "bookrent-nats",
  "version": "1.0.0",
  "description": "",
  "infra": "monorepo",
  "private": true,
  "scripts": {
    "node:version": "node --version",
    "pnpm:version": "pnpm --version",
    "pnpm:yourpackage": "pnpm view yourpackage versions",
    "pnpm:clean": "find ./ -name node_modules -type d -exec rm -rf {} +",
    "pnpm:update": "pnpm -r update -i -L",
    "pnpm:dedupe": "pnpm dedupe --check",
    "moon:clean": "moon clean --lifetime '7 days'",
    "moon": "moon  trace --color --log",
    "deps": "pnpm update -i --latest -r",
    "m:graph": "moon project-graph",
    "check": "moon check --all",
    "format": "moon run :format",
    "typecheck": "moon run :typecheck",
    "test": "moon run :test",
    "lint": "moon run :lint",
    "dev": "moon run :dev",
    "build": "moon run :build",
    "app": "moon run :task --query 'projectType=application'",
    "http-api-gateway": "moon run http-api-gateway:build",
    "users-microservice": "moon run users-microservice:build",
    "orders-microservice": "moon run orders-microservice:build",
    "http-api-gateway:dev": "moon run http-api-gateway:dev",
    "users-microservice:dev": "moon run http-api-gateway:dev",
    "orders-microservice:dev": "moon run http-api-gateway:dev",
    "http-api-gateway:m:graph": "moon project-graph http-api-gateway",
    "users-microservice:m:graph": "moon project-graph users-microservice",
    "orders-microservice:m:graph": "moon project-graph orders-microservice",
    "typescript": "moon run :build --query \"language=typescript\""
  },
  "keywords": [],
  "author": "Arman Riazi",
  "license": "ISC",
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "@moonrepo/cli": "^1.19.0",
    "@swc/core": "^1.3.102",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "babel-preset-moon": "^2.0.13",
    "eslint": "^8.56.0",
    "eslint-config-moon": "^2.0.14",
    "jest": "^29.7.0",
    "jest-preset-moon": "^2.0.6",
    "packemon": "^3.2.3",
    "prettier": "^3.1.1",
    "prettier-config-moon": "^1.1.2",
    "prettier-plugin-svelte": "^3.1.2",
    "tsconfig-moon": "^1.3.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "node": "18.18.2"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0"
  }
}
